{
  "expo": {
    "name": "SoundBoardApp",
    "slug": "SoundBoardApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "CAMERA",
          "RECORD_AUDIO",
          "PLAY_AUDIO"
        ]
      }
      
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
